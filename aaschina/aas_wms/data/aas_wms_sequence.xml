<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_aas_product_label" model="ir.sequence">
            <field name="name">产品标签</field>
            <field name="code">aas.product.label</field>
            <field name="prefix">AC%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_aas_stock_delivery" model="ir.sequence">
            <field name="name">发货单号</field>
            <field name="code">aas.stock.delivery</field>
            <field name="prefix">AD%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_aas_stock_receipt" model="ir.sequence">
            <field name="name">收货单号</field>
            <field name="code">aas.stock.receipt</field>
            <field name="prefix">AE%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_aas_receive_deliver" model="ir.sequence">
            <field name="name">收发汇总</field>
            <field name="code">aas.receive.deliver</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_aas_stock_inventory" model="ir.sequence">
            <field name="name">库存盘点</field>
            <field name="code">aas.stock.inventory</field>
            <field name="prefix">AH%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>