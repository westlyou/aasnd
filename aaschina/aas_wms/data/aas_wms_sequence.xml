<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_aas_product_label" model="ir.sequence">
            <field name="name">产品标签</field>
            <field name="code">aas.product.label</field>
            <field name="prefix">AB%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
        <record id="seq_aas_receive_deliver" model="ir.sequence">
            <field name="name">收发汇总</field>
            <field name="code">aas.receive.deliver</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>