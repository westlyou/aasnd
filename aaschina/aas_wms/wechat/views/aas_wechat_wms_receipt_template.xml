<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <template id="wechat_wms_receipt_line_list" name="Wechat WMS Receipt Line List">
            &lt;!DOCTYPE html&gt;
            <html>
                <head>
                    <meta charset="utf-8" />
                    <title>安费诺先进传感器</title>
                    <meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no"/>
                    <meta name="apple-mobile-web-app-capable" content="yes"/>
                    <meta name="apple-mobile-web-app-status-bar-style" content="black"/>
                    <link rel="stylesheet" href="/aas_base/static/wechat/mui/css/mui.min.css"/>
                    <link rel="stylesheet" href="/aas_base/static/wechat/iconfont/iconfont.css"/>
                    <link rel="stylesheet" href="/aas_base/static/wechat/aas/css/wechat.css"/>
                </head>
                <body>
                    <header class="mui-bar mui-bar-nav">
                        <h1 class="mui-title"> 收货上架 </h1>
                    </header>
                    <div id="receipt_line_list_pullrefresh" class="mui-content mui-scroll-wrapper" t-att-lineindex="lineindex">
                        <div class="mui-scroll">
                            <div class="mui-input-row mui-search" style="height:34px;margin:10px;">
                                <input id="product_code_search" type="search" class="mui-input-clear" placeholder="请输入产品编码"/>
                            </div>
                            <ul id="receipt_line_list" class="mui-table-view mui-table-view-chevron">
                                <li class="mui-table-view-cell">
                                    <div class='mui-table'>
                                        <div class='mui-table-cell mui-col-xs-8 mui-text-left'>产品编码</div>
                                        <div class='mui-table-cell mui-col-xs-4 mui-text-right'>收货类型</div>
                                    </div>
                                    <div class='mui-table'>
                                        <div class='mui-table-cell mui-col-xs-8 mui-text-left'>收货单号</div>
                                        <div class='mui-table-cell mui-col-xs-4 mui-text-right'>推荐库位</div>
                                    </div>
                                </li>
                                <t t-if="receiptlines">
                                   <t t-foreach="receiptlines" t-as="rline">
                                       <li class="aas-receipt-line mui-table-view-cell" t-att-receiptlineid="rline['line_id']">
                                           <a class='mui-navigate-right' style='padding-right:40px;' href='javascript:;'>
                                               <div class='mui-table'>
                                                   <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="rline['product_code']"/> </div>
                                                   <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="rline['receipt_type']"/> </div>
                                               </div>
                                               <div class='mui-table'>
                                                   <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="rline['receipt_name']"/> </div>
                                                   <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="rline['location_name']"/> </div>
                                               </div>
                                           </a>
                                       </li>
                                   </t>
                                </t>
                            </ul>
                        </div>
                    </div>
                </body>
                <script type="text/javascript" src="/aas_base/static/wechat/mui/js/mui.min.js"></script>
                <script type="text/javascript" src="/aas_wms/static/wechat/wechat_receipt_line_list.js"></script>
            </html>
        </template>

        <template id="wechat_wms_receipt_line_detail" name="Wechat WMS Receipt Line Detail">
            &lt;!DOCTYPE html&gt;
            <html>
                <head>
                    <meta charset="utf-8" />
                    <title>安费诺先进传感器</title>
                    <meta name="viewport" content="width=device-width, initial-scale=1,maximum-scale=1,user-scalable=no"/>
                    <meta name="apple-mobile-web-app-capable" content="yes"/>
                    <meta name="apple-mobile-web-app-status-bar-style" content="black"/>
                    <link rel="stylesheet" href="/aas_base/static/wechat/mui/css/mui.min.css"/>
                    <link rel="stylesheet" href="/aas_base/static/wechat/iconfont/iconfont.css"/>
                    <link rel="stylesheet" href="/aas_base/static/wechat/aas/css/wechat.css"/>
                </head>
                <body>
                    <header class="mui-bar mui-bar-nav">
                        <a class="mui-icon mui-icon-bars mui-pull-right" style="color: #0870d9;" href="#receipt_line_buttons"></a>
                        <h1 class="mui-title"> 收货详情 </h1>
                    </header>
                    <div id="receipt_line_detail_pullrefresh" class="mui-content mui-scroll-wrapper" t-att-lineid="receiptlineid">
                        <div class="mui-scroll">
                            <ul class="mui-table-view">
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">收货单号</span>
                                    <span class="mui-pull-right"> <t t-esc="receipt_name"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">收货类型</span>
                                    <span class="mui-pull-right"> <t t-esc="type_name"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">收货状态</span>
                                    <span class="mui-pull-right"> <t t-esc="state_name"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">来源单据</span>
                                    <span class="mui-pull-right"> <t t-esc="origin_order"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">产品编码</span>
                                    <span class="mui-pull-right"> <t t-esc="product_code"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">产品单位</span>
                                    <span class="mui-pull-right"> <t t-esc="product_uom"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">应收数量</span>
                                    <span class="mui-pull-right"> <t t-esc="product_qty"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">已收数量</span>
                                    <span class="mui-pull-right"> <t t-esc="receipt_qty"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">处理中数量</span>
                                    <span id="receipt_doing_qty" class="mui-pull-right"> <t t-esc="doing_qty"/> </span>
                                </li>
                                <li class="mui-table-view-cell">
                                    <span class="mui-pull-left">上架库位</span>
                                    <span id="push_location" class="mui-pull-right" t-att-locationid="push_location_id">
                                        <t t-esc="push_location_name"/>
                                    </span>
                                </li>
                            </ul>
                            <div class="aas-mui-table-view-title"> 收货标签 </div>
                            <ul id="receipt_labels" class="mui-table-view mui-table-view-chevron">
                                <li class="mui-table-view-cell">
                                    <div class='mui-table'>
                                        <div class='mui-table-cell mui-col-xs-8 mui-text-left'>标签名称</div>
                                        <div class='mui-table-cell mui-col-xs-4 mui-text-right'>产品批次</div>
                                    </div>
                                    <div class='mui-table'>
                                        <div class='mui-table-cell mui-col-xs-8 mui-text-left'>产品编码</div>
                                        <div class='mui-table-cell mui-col-xs-4 mui-text-right'>产品数量</div>
                                    </div>
                                </li>
                                <t t-if="labellist">
                                    <t t-foreach="labellist" t-as="rlabel">
                                        <li t-if="rlabel['checked']" class="mui-table-view-cell" t-attf-id="label_{{rlabel['label_id']}}">
                                            <div class='mui-table'>
                                                <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="rlabel['label_name']"/> </div>
                                                <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="rlabel['product_lot']"/> </div>
                                            </div>
                                            <div class='mui-table'>
                                                <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="rlabel['product_code']"/> </div>
                                                <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="rlabel['product_qty']"/> </div>
                                            </div>
                                        </li>
                                        <li t-if="not rlabel['checked']" class="mui-table-view-cell aas-receipt-label" t-attf-id="label_{{rlabel['label_id']}}">
                                            <div class='mui-table'>
                                                <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="rlabel['label_name']"/> </div>
                                                <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="rlabel['product_lot']"/> </div>
                                            </div>
                                            <div class='mui-table'>
                                                <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="rlabel['product_code']"/> </div>
                                                <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="rlabel['product_qty']"/> </div>
                                            </div>
                                        </li>
                                    </t>
                                </t>
                            </ul>
                            <div class="aas-mui-table-view-title"> 上架作业 </div>
                            <ul id="receipt_operations" class="mui-table-view mui-table-view-chevron">
                                <li class="mui-table-view-cell">
                                    <div class='mui-table'>
                                        <div class='mui-table-cell mui-col-xs-8 mui-text-left'>标签名称</div>
                                        <div class='mui-table-cell mui-col-xs-4 mui-text-right'>产品批次</div>
                                    </div>
                                    <div class='mui-table'>
                                        <div class='mui-table-cell mui-col-xs-8 mui-text-left'>产品编码</div>
                                        <div class='mui-table-cell mui-col-xs-4 mui-text-right'>产品数量</div>
                                    </div>
                                </li>
                                <t t-if="operationlist">
                                    <t t-foreach="operationlist" t-as="roperation">
                                        <li t-if="not roperation['push_onshelf']" class="mui-table-view-cell"
                                            t-attf-labelid="label_{{roperation['label_id']}}" t-att-operationid="roperation['operation_id']">
                                            <div class="mui-slider-right mui-disabled">
                                                <a class="mui-btn mui-btn-red">删除</a>
                                            </div>
                                            <div class="mui-slider-handle">
                                                <div class='mui-table'>
                                                    <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="roperation['label_name']"/> </div>
                                                    <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="roperation['product_lot']"/> </div>
                                                </div>
                                                <div class='mui-table'>
                                                    <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="roperation['product_code']"/> </div>
                                                    <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="roperation['product_qty']"/> </div>
                                                </div>
                                            </div>
                                        </li>
                                        <li t-if="roperation['push_onshelf']" class="mui-table-view-cell"
                                            t-attf-labelid="label_{{roperation['label_id']}}" t-att-operationid="roperation['operation_id']">
                                            <div class='mui-table'>
                                                <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="roperation['label_name']"/> </div>
                                                <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="roperation['product_lot']"/> </div>
                                            </div>
                                            <div class='mui-table'>
                                                <div class='mui-table-cell mui-col-xs-8 mui-text-left'> <t t-esc="roperation['product_code']"/> </div>
                                                <div class='mui-table-cell mui-col-xs-4 mui-text-right'> <t t-esc="roperation['product_qty']"/> </div>
                                            </div>
                                        </li>
                                    </t>
                                </t>
                            </ul>
                        </div>
                    </div>
                    <div id="receipt_line_buttons" class="mui-popover mui-popover-action mui-popover-bottom">
                        <ul class="mui-table-view">
                            <li class="mui-table-view-cell"> <a id="action_change_location" href="javascript:;">扫描库位</a></li>
                            <li class="mui-table-view-cell" t-attf-style="display:{{labelscan}}"> <a id="action_scan_label" href="javascript:;">扫描标签</a></li>
                            <li class="mui-table-view-cell" t-attf-style="display:{{pushall}}"> <a id="action_push_all" href="javascript:;">批量上架</a></li>
                            <li class="mui-table-view-cell" t-attf-style="display:{{pushdone}}" id="action_push_done"> <a href="javascript:;">上架完成</a></li>
                        </ul>
                        <ul class="mui-table-view">
                            <li class="mui-table-view-cell"> <a href="#receipt_line_buttons"><b>取消</b></a> </li>
                        </ul>
                    </div>
                </body>
                <script type="text/javascript" src="/aas_base/static/wechat/mui/js/mui.min.js"></script>
                <script type="text/javascript" src="http://res.wx.qq.com/open/js/jweixin-1.1.0.js"></script>
                <script type="text/javascript" src="/aas_wms/static/wechat/wechat_receipt_line_detail.js"></script>
            </html>
        </template>

    </data>
</odoo>