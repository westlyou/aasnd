<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="view_tree_aas_stock_delivery_inside" model="ir.ui.view">
            <field name="name">aas.stock.delivery.inside.tree</field>
            <field name="model">aas.stock.delivery</field>
            <field name="type">tree</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="order_user"/>
                    <field name="order_time"/>
                    <field name="origin_order"/>
                    <field name="partner_id"/>
                    <field name="state"/>
                    <field name="done_time"/>
                </tree>
            </field>
        </record>

        <record id="view_form_aas_stock_delivery_inside" model="ir.ui.view">
            <field name="name">aas.stock.delivery.inside.form</field>
            <field name="model">aas.stock.delivery</field>
            <field name="type">form</field>
            <field name="arch" type="xml">
                <form>
                    <header>
                        <button name="action_confirm" string="确认订单" type="object" class="oe_highlight" states="draft"/>
                        <button name="action_picking_list" string="拣货清单" type="object" class="oe_highlight" states="confirm,picking"/>
                        <button name="action_picking_confirm" string="确认拣货" type="object" class="oe_highlight"
                                attrs="{'invisible':['|',('state','not in',['confirm','picking']),('picking_confirm','=',True)]}"/>
                        <button name="action_deliver_done" string="确认发货" type="object" class="oe_highlight"
                                attrs="{'invisible':[('picking_confirm','=',False)]}"/>
                        <field name="state" widget="statusbar" statusbar_visible="draft,confirm,picking,done,cancel"/>
                    </header>
                    <sheet>
                        <group>
                            <group>
                                <field name="name" readonly="1"/>
                                <field name="order_user" readonly="1"/>
                                <field name="order_time" readonly="1"/>
                            </group>
                            <group>
                                <field name="origin_order" readonly="1"/>
                                <field name="delivery_type" readonly="1"/>
                                <field name="done_time" readonly="1"/>
                            </group>
                        </group>
                        <group>
                            <group>
                                <field name="location_id"/>
                                <field name="partner_id"/>
                                <field name="done_time" readonly="1"/>
                                <field name="picking_confirm" invisible="1"/>
                            </group>
                        </group>
                        <notebook>
                            <page string="发货明细">
                                <field name="delivery_lines"
                                       attrs="{'readonly':['|',('state','!=','draft'),('delivery_type','=','purchase')]}">
                                    <tree editable="bottom">
                                        <field name="product_id" options="{'no_create':true}" required="1"/>
                                        <field name="product_uom" readonly="1"/>
                                        <field name="product_qty" required="1"/>
                                        <field name="delivery_qty" readonly="1"/>
                                        <field name="picking_qty" readonly="1"/>
                                        <field name="state" readonly="1"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <group>
                                                <field name="product_id"/>
                                                <field name="product_uom"/>
                                                <field name="state"/>
                                            </group>
                                            <group>
                                                <field name="product_qty"/>
                                                <field name="delivery_qty"/>
                                                <field name="picking_qty"/>
                                            </group>
                                        </group>
                                    </form>
                                </field>
                            </page>
                            <page string="拣货清单">
                                <field name="picking_list" readonly="1">
                                    <tree>
                                        <field name="product_id"/>
                                        <field name="product_lot"/>
                                        <field name="product_qty"/>
                                        <field name="location_id"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <group>
                                                <field name="product_id"/>
                                                <field name="product_qty"/>
                                            </group>
                                            <group>
                                                <field name="product_lot"/>
                                                <field name="location_id"/>
                                            </group>
                                        </group>
                                    </form>
                                </field>
                            </page>
                            <page string="发货作业">
                                <field name="operation_lines" attrs="{'readonly':['|',('state','in',['draft','done','cancel']),('picking_confirm','=',True)]}">
                                    <tree editable="bottom" edit="false">
                                        <field name="label_id" options="{'no_create':true}" required="1"
                                                domain="[('parent_id','=',False),('locked','=',False),('state','=','normal')]"/>
                                        <field name="product_id" readonly="1"/>
                                        <field name="product_uom" readonly="1"/>
                                        <field name="product_lot" readonly="1"/>
                                        <field name="product_qty" readonly="1"/>
                                        <field name="location_id" readonly="1"/>
                                        <field name="pick_user" readonly="1"/>
                                        <field name="pick_time" readonly="1"/>
                                        <field name="deliver_done" readonly="1"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <group> <field name="label_id"/> </group>
                                        </group>
                                        <group>
                                            <group>
                                                <field name="product_id"/>
                                                <field name="product_uom"/>
                                                <field name="product_lot"/>
                                                <field name="pick_user"/>
                                            </group>
                                            <group>
                                                <field name="product_qty"/>
                                                <field name="location_id"/>
                                                <field name="deliver_done"/>
                                                <field name="pick_time"/>
                                            </group>
                                        </group>
                                    </form>
                                </field>
                            </page>
                            <page string="执行明细">
                                <field name="move_lines" readonly="1">
                                    <tree>
                                        <field name="product_id"/>
                                        <field name="product_uom"/>
                                        <field name="product_lot"/>
                                        <field name="product_qty"/>
                                        <field name="delivery_user"/>
                                        <field name="delivery_time"/>
                                        <field name="location_src_id"/>
                                        <field name="location_dest_id"/>
                                    </tree>
                                    <form>
                                        <group>
                                            <field name="product_id"/>
                                            <field name="product_uom"/>
                                            <field name="product_lot"/>
                                            <field name="product_qty"/>
                                        </group>
                                        <group>
                                            <field name="delivery_user"/>
                                            <field name="delivery_time"/>
                                            <field name="location_src_id"/>
                                            <field name="location_dest_id"/>
                                        </group>
                                    </form>
                                </field>
                            </page>
                        </notebook>
                    </sheet>
                </form>
            </field>
        </record>

