                                        <field name="location_id" readonly="1"/>
                                        <field name="pick_user" readonly="1"/>
                                        <field name="pick_time" readonly="1"/>
                                        <field name="check_user" readonly="1"/>
                                        <field name="check_time" readonly="1"/>
