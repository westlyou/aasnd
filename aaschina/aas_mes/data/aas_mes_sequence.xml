<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="seq_aas_mes_wireorder" model="ir.sequence">
            <field name="name">线材工单</field>
            <field name="code">aas.mes.wireorder</field>
            <field name="prefix">AU%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_aas_mes_allocation" model="ir.sequence">
            <field name="name">生产调拨</field>
            <field name="code">aas.mes.allocation</field>
            <field name="prefix">%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_aas_mes_producttest_order" model="ir.sequence">
            <field name="name">检测工单</field>
            <field name="code">aas.mes.producttest.order</field>
            <field name="prefix">FAE%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">5</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>