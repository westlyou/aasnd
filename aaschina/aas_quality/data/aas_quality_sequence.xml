<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="seq_aas_quality_order" model="ir.sequence">
            <field name="name">质检单号</field>
            <field name="code">aas.quality.order</field>
            <field name="prefix">AG%(year)s%(month)s%(day)s</field>
            <field name="number_increment">1</field>
            <field name="number_next">1</field>
            <field name="padding">4</field>
            <field name="loop_type">day</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>